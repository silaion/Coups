<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/RelativeLayout1"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="vertical" >

    <ImageView
        android:id="@+id/imageView1"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_alignParentTop="true"
        android:scaleType="fitXY"
        android:src="@drawable/abc_cab_background_bottom_holo_light" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/c_Number"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentTop="true"
        android:scaleType="fitXY"
        android:src="@drawable/bar1" />

    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:text="도장 쿠폰"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:textColor="#000000"
        android:textSize="25dp" />

    <TextView
        android:id="@+id/c_Number"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/textView1"
        android:layout_marginLeft="24dp"
        android:layout_marginTop="14dp"
        android:text="회원번호"
        android:textColor="#000000" />

    <Button
        android:id="@+id/discouponbuy"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_marginBottom="20dp"
        android:layout_marginRight="20dp"
        android:text="할인쿠폰 구매하기" />

    <Button
        android:id="@+id/benefit"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/s_Name"
        android:layout_toRightOf="@+id/textView1"
        android:text="혜택 정보" />

    <TextView
        android:id="@+id/s_Name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/c_Number"
        android:layout_below="@+id/c_Number"
        android:layout_marginTop="24dp"
        android:text="한경 카페"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:textColor="#000000"
        android:textSize="30dp" />

    <TextView
        android:id="@+id/s_Addr"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/benefit"
        android:layout_marginTop="15dp"
        android:text="경기도 안성시 한경대학교"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:textColor="#000000"
        android:textSize="20dp" />

    <TextView
        android:id="@+id/s_Due_date"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/s_Addr"
        android:text="유효기간(2014.9.1~2014.12.31)"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="#000000"
        android:textSize="20dp" />

    <TextView
        android:id="@+id/s_All_coup"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@id/c_Number"
        android:layout_alignRight="@+id/benefit"
        android:text="다 모은 쿠폰 : "
        android:layout_below="@id/benefit"/>

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/discouponbuy"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/s_Due_date"
        android:layout_marginTop="56dp"
        android:scaleType="fitXY"
        android:src="@drawable/box3" />

    <ImageView
        android:id="@+id/ImageView01"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_alignParentLeft="true"
        android:layout_alignTop="@+id/imageView3"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="34dp"
        android:scaleType="fitXY"
        android:visibility="invisible"
        android:src="@drawable/icon" />

    <ImageView
        android:id="@+id/ImageView02"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_alignTop="@+id/ImageView01"
        android:layout_marginLeft="40dp"
        android:layout_toRightOf="@+id/ImageView01"
        android:scaleType="fitXY"
        android:visibility="invisible"
        android:src="@drawable/icon" />

    <ImageView
        android:id="@+id/ImageView03"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_alignTop="@+id/ImageView02"
        android:layout_centerHorizontal="true"
        android:layout_marginLeft="15dp"
        android:scaleType="fitXY"
        android:visibility="invisible"
        android:src="@drawable/icon" />

    <ImageView
        android:id="@+id/ImageView04"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_alignTop="@+id/ImageView03"
        android:layout_toRightOf="@+id/textView1"
        android:layout_marginLeft="5dp"
        android:scaleType="fitXY"
        android:visibility="invisible"
        android:src="@drawable/icon" />

    <ImageView
        android:id="@+id/ImageView05"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_alignParentRight="true"
        android:layout_alignTop="@+id/ImageView04"
        android:layout_marginRight="20dp"
        android:scaleType="fitXY"
        android:visibility="invisible"
        android:src="@drawable/icon" />

    <ImageView
        android:id="@+id/ImageView06"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_alignBottom="@+id/imageView3"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="15dp"
        android:layout_marginBottom="30dp"
        android:scaleType="fitXY"
        android:visibility="invisible"
        android:src="@drawable/icon" />

    <ImageView
        android:id="@+id/ImageView07"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_alignRight="@+id/ImageView02"
        android:layout_alignTop="@+id/ImageView06"
        android:layout_marginLeft="30dp"
        android:scaleType="fitXY"
        android:visibility="invisible"
        android:src="@drawable/icon" />

    <ImageView
        android:id="@+id/ImageView08"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_alignLeft="@+id/ImageView03"
        android:layout_alignTop="@+id/ImageView07"
        android:scaleType="fitXY"
        android:visibility="invisible"
        android:src="@drawable/icon" />

    <ImageView
        android:id="@+id/ImageView09"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_alignTop="@+id/ImageView08"
        android:layout_toRightOf="@+id/textView1"
        android:scaleType="fitXY"
        android:visibility="invisible"
        android:src="@drawable/icon" />

    <ImageView
        android:id="@+id/ImageView10"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_alignParentRight="true"
        android:layout_alignTop="@+id/ImageView09"
        android:layout_marginRight="20dp"
        android:scaleType="fitXY"
        android:visibility="invisible"
        android:src="@drawable/icon" />

</RelativeLayout>